{% extends 'base.html.twig' %}
{% block body %}
    <h1>Competences index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for competence in competences %}
            <tr>
                <td>{{ competence.id }}</td>
                <td>{{ competence.nom }}</td>
                <td>
                    <a href="{{ path('app_competences_show', {'id': competence.id}) }}">show</a>
                    <a href="{{ path('app_competences_edit', {'id': competence.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_competences_new') }}">Create new</a>
{% endblock %}
